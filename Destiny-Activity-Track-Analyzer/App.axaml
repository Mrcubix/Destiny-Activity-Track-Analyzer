<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Tracker"
             x:Class="Tracker.App">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>
        <StyleInclude Source="/Controls/Templates/UXSettingsControl.axaml" />
        <StyleInclude Source="/Controls/Templates/APISettingsControl.axaml" />
    </Application.Styles>

    <Application.Styles>
        <FluentTheme Mode="Dark"/>
    </Application.Styles>
</Application>
